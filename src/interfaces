auto lo
iface lo inet loopback

# Data network
# already configured during network boot
auto eth0
iface eth0 inet static
address 192.168.0.50
netmask 255.255.255.0

# Control network
auto eth1
iface eth1 inet static
address 192.168.1.50
netmask 255.255.255.0

